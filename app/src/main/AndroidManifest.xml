<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="citizenone.lalax.com.citizenone">

    <application
        android:name="com.lalax.citizenone.models.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_burger_transparent"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.lalax.citizenone.controllers.MainMenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.lalax.citizenone.controllers.QuestionnaireActivity" />
        <activity android:name="com.lalax.citizenone.controllers.ProgressActivity" />
        <activity
            android:name="com.lalax.citizenone.controllers.DifficultyActivity"
            android:label="@string/title_activity_difficulty" />
    </application>

</manifest>